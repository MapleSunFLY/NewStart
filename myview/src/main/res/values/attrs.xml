<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SaleProgressView">
        <attr name="sideColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="sideWidth" format="dimension" />
        <attr name="overText" format="string" />
        <attr name="nearOverText" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="isNeedAnim" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ArcProgress">
        <attr name="arcFinishedColor" format="color" />
        <attr name="arcUnfinishedColor" format="color" />
        <attr name="arcProgressMax" format="integer" />
        <attr name="arcProgress" format="integer" />
        <attr name="arcFullDegree" format="integer" />
        <attr name="arcWidth" format="integer" />
        <attr name="animaTime" format="integer" />
        <attr name="draggingEnabled" format="boolean" />
        <attr name="describeText" format="string" />
        <attr name="describeTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ArcMatrixProgress">
        <attr name="arcMatrixFinishedColor" format="color" />
        <attr name="arcMatrixUnfinishedColor" format="color" />
        <attr name="arcMatrixOwnDegree" format="integer" />
        <attr name="arcMatrixWitch" format="integer" />
        <attr name="arcMatrixProgressMax" format="integer" />
        <attr name="arcMatrixProgress" format="integer" />
    </declare-styleable>

    <declare-styleable name="UpdataAPPProgressBar">
        <attr name="updataAPPReachedBarColor" format="color" />
        <attr name="updataAPPUnreachedBarColor" format="color" />
        <attr name="updataAPPTextColor" format="color" />
        <attr name="updataAPPBarHeight" format="dimension" />
        <attr name="updataAPPMax" format="integer" />
        <attr name="updataAPPProgress" format="integer" />
        <attr name="updataAPPSuffix" format="string" />
        <attr name="updataAPPPrefix" format="string" />
        <attr name="updataAPPTextVisibility" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StageProgress">
        <attr name="stageReachedBarColor" format="color" />
        <attr name="stageUnreachedBarColor" format="color" />
        <attr name="stageUnreachedTextColor" format="color" />
        <attr name="stageBarHeight" format="dimension" />
        <attr name="stageMax" format="integer" />
        <attr name="stageProgress" format="integer" />
        <attr name="stageAllStage" format="integer" />
        <attr name="stageThisStage" format="integer" />
        <attr name="stageText" format="string" />
        <attr name="stageTextColor" format="color" />
        <attr name="stageTextBackgroundColor" format="color" />
        <attr name="stageTextVisibility" format="boolean" />
        <attr name="stageUnreachedBarBitmap" format="reference"/>
    </declare-styleable>
</resources>